    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "moduleDetection": "force",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "outDir": "./dist"
    },
    "include": [
        "src",
        "__test__"
    ]
}